<div id="Low-Price-Type-Mobile-Collection" class="mobile-collection collectionGrid container-wrap js-collection-grid js-collectionGrid" id="collection-module-{{ section.id }}" data-show-quickview="{{ settings.show_quick_view }}" data-show_reviews="{{ show_reviews }}" data-show_quantity="{{ settings.show_quantity }}" data-color_swatches="{{ color_swatches }}" data-carousel="{{ enable_carousel }}" data-slides-per-row="{{ settings.products_columns }}" data-grid-size="{{ settings.grid_size }}">
    <div class="collection-slider__wrapper">
        <div class="bf-slider" id="low-price-mobile-slider">
            {% for product in section.settings.low-price.products   %}
                {% include 'collectionGrid-block' %}
            {% endfor %}
        </div>
    </div>
</div><script>
  $(document).ready(() => {
      $('#low-price-mobile-slider').not('slick-initialized').slick({
          infinite: false,
          slidesToShow: 1,
          slidesToScroll: 1,
          nextArrow: $('.Mobile-Arrow-Button-Four-Next'),
          prevArrow: $('.Mobile-Arrow-Button-Four-Prev')
      });
  })
</script>